{
	"compilerOptions": {
		"outDir": ".",
		"target": "ESNext",
		"module": "CommonJS",
		"strict": true,
		"rootDir": "src",
		"declaration": true,
		"noUnusedLocals": true,
		"noImplicitAny": false,
		"noUnusedParameters": true,
		"allowUnusedLabels": false,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"importsNotUsedAsValues": "error",
		"sourceMap": true,
		"inlineSources": true
	},
	"exclude": [],
	"include": [
		"src/*.ts"
	]
}
